
{
  "manifest_version": 2,
  "name": "SVG Extractor Pro",
  "version": "1.0",
  "description": "Extract and save SVG images from websites",
  "permissions": ["activeTab", "downloads", "<all_urls>", "storage"],
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "SVG Extractor Pro"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": [
    "results.html",
    "results.js",
    "results.css"
  ]
}
